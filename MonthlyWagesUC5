import java.util.Random;
public class WageSwitchUC4
{
  public static void main(String args[])
  {
    int a=1, emphours, empRate=20, workdays=20;
    Random x = new Random();
    for(int i=1;i<=workdays;i++)
    {
      Random choice = new Random();
      {
        switch(choice)
        {
          case 0:
            emphours=8;
            break;
          case 1:
            emphours=4;
            break;
          case 2:
            emphours=0;
            break;
        }
     }
      salary=(emphours*empRate);
      System.out.println(salary);
    }
  }
}
